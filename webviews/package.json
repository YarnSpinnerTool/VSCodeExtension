{
    "name": "yarnspinner-vscode-graph-view",
    "version": "0.0.1",
    "type": "module",
    "private": true,
    "scripts": {
        "start": "vite",
        "build": "tsc --noEmit && vite build --mode graph-view:production && vite build --mode preview:production",
        "preview": "vite preview",
        "watch:graph-view": "NODE_ENV=development vite build --mode graph-view:development --watch",
        "watch:preview": "NODE_ENV=development vite build --mode preview:development --watch"
    },
    "dependencies": {
        "@vscode/webview-ui-toolkit": "^1.4.0",
        "@xyflow/react": "^12.8.2",
        "@yarnspinnertool/core": "^0.0.1-132",
        "clsx": "^2.1.1",
        "elkjs": "^0.10.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-error-boundary": "^6.0.0",
        "vite-plugin-svgr": "^4.3.0",
        "zod": "^4.0.13",
        "zustand": "^5.0.8"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.1.11",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@types/vscode-webview": "^1.57.0",
        "@vitejs/plugin-react": "^4.7.0",
        "eslint": "^9.31.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "prettier-plugin-tailwindcss": "^0.7.0",
        "tailwindcss": "^4.1.11",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.37.0",
        "vite": "^7.0.5",
        "vite-bundle-analyzer": "^1.2.3",
        "vite-tsconfig-paths": "^5.1.4"
    }
}
